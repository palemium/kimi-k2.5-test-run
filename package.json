{
  "name": "Pythagoras",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "jscpd": "jscpd",
    "knip": "knip",
    "knip:fix": "knip --fix --allow-remove-files",
    "quality": "npm run typecheck && npm run lint && npm run format:check && npm run jscpd && npm run knip",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "kysely": "^0.28.10",
    "next": "16.1.6",
    "pg": "^8.17.2",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "eslint-plugin-sonarjs": "^3.0.6",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^17.2.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "jscpd": "^4.0.8",
    "knip": "^5.82.1",
    "lefthook": "^2.0.16",
    "postcss": "^8",
    "prettier": "^3.8.1",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
